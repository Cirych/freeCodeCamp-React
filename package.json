{
  "name": "freecodecamp-react",
  "version": "1.0.0",
  "description": "React Projects for freeCodeCamp",
  "main": "index.js",
  "scripts": {
    "test": "npm bin && mocha --compilers js:babel-core/register --require ./test/test_helper.js",
    "test:watch": "npm run test -- --watch",
    "build": "npm bin && webpack",
    "dev": "npm bin && webpack-dev-server",
    "start": "npm run dev",
    "deploy": "npm bin && webpack --progress -p --config webpack.production.config.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cirych/freeCodeCamp-React.git"
  },
  "keywords": [
    "freeCodeCamp",
    "React Markdown Previewer",
    "Camper Leaderboard",
    "Recipe Box",
    "Game of Life",
    "Roguelike Dungeon Crawler Game"
  ],
  "author": "Cirych",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cirych/freeCodeCamp-React/issues"
  },
  "homepage": "https://github.com/Cirych/freeCodeCamp-React#readme",
  "dependencies": {
